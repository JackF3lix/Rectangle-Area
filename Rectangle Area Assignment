  perimeter = 2 * (length * width)

  cout << "Rectangle Properties:" << endl;
  cout << "Length = " << length << endl;
  cout << "Width = " << width << endl;
  cout << "Area = " << area <, endl;
  cout << "Perimeter = " << permieter << endl;
